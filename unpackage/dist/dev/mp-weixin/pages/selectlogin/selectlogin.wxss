.select-login-box {
  width: 100%;
  height: 100%;
}
.select-login-box .select-login-pad {
    width: 100%;
    height: 100%;
    padding: 0 20rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
}
.select-login-box .select-login-pad .login-logo-image {
      width: 320rpx;
      height: 192rpx;
      margin: 173rpx auto 0;
}
.select-login-box .select-login-pad .login-logo-image image {
        width: 100%;
        height: 100%;
        display: block;
}
.select-login-box .select-login-pad .login-but {
      width: 100%;
      height: 80rpx;
      background: #038577;
      border-radius: 40rpx;
      font-size: 30rpx;
      color: white;
      line-height: 80rpx;
      text-align: center;
      margin-top: 260rpx;
}
.select-login-box .select-login-pad .login-phone-register {
      font-size: 30rpx;
      color: #333333;
      line-height: 30rpx;
      margin-top: 70rpx;
      margin-bottom: 73rpx;
      text-align: center;
}
.select-login-box .select-login-pad .clear-login {
      font-size: 26rpx;
      color: #9b9b9b;
      line-height: 26rpx;
      text-align: center;
}

